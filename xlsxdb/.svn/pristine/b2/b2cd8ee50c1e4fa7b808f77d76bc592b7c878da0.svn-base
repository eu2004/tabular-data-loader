package ro.eu.xlsxdb.xlsxloader;

/**
 * Created by emilu on 5/21/2016.
 */
public class XLSXColumn {
    private String name;
    private XLSXColumnType type;
    private int index;

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public XLSXColumnType getType() {
        return type;
    }

    public void setType(XLSXColumnType type) {
        this.type = type;
    }

    public int getIndex() {
        return index;
    }

    public void setIndex(int index) {
        this.index = index;
    }

	@Override
	public String toString() {
		return "XLSXColumn [name=" + name + ", type=" + type + ", index=" + index + "]";
	}
}
